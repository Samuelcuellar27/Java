import java.util.Scanner;

public class java
{
    public static void main(String[] args){
        Scanner tec = new Scanner(System.in);
        System.out.println("Type your age");
        int x = tec.nextInt();
        if (x >= 18){
            System.out.println("can vote");
        }
        else{
            System.out.println("can't vote");
        }
    }
}
